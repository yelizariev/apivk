<?xml version="1.0" encoding="utf-8"?>
<project name="apivk example with queue" basedir="." default="swf">
	<property file="../../build.properties" />
	<property
		 name="lib.dir"
		 value="../../lib"
		 />
	<property
		 name="apivk.standalone.swc"
		 value="apivk_standalone.swc"
		 />
	<taskdef resource="flexTasks.tasks" classpath="${flex.ant.tasks}" />
	<target name="apivk-sa">
		<ant
			 antfile="../../build.xml"
			 target="swc-sa"
			 inheritAll="false"
			 />
	</target>
	<target
		 name="swf"
		 depends="apivk-sa"
		 >
		<mxmlc
			 file="src/Queue.as"
			 output="bin/Queue.swf"
			 >
			<load-config filename="${flex.config}" />
			<source-path path-element="src"/>
			<library-path
				 dir="${lib.dir}"
				 includes="${apivk.standalone.swc}"
				 />
		</mxmlc>
	</target>
</project>
